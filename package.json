{
  "name": "Bootstrap-Admin-Template",
  "description": "Free Admin Template Based On Twitter Bootstrap 3.x",
  "version": "2.3.2",
  "homepage": "https://github.com/puikinsh/Bootstrap-Admin-Template",
  "author": {
    "name": "puikinsh",
    "url": "http://github.com/puikinsh/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/puikinsh/Bootstrap-Admin-Template.git"
  },
  "bugs": {
    "url": "https://github.com/puikinsh/Bootstrap-Admin-Template/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/puikinsh/Bootstrap-Admin-Template/blob/master/LICENSE.md"
  },
  "keywords": [
    "twbs",
    "assemble",
    "grunt",
    "less",
    "admin",
    "template"
  ],
  "main": "Gruntfile.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "html": "assemble",
    "bower": "bower install",
    "grunt": "grunt",
    "build": "npm run bower && npm run grunt && npm run html",
    "mainrtl": "css-flip dist/assets/css/main.css > dist/assets/css/main.rtl.css",
    "bootstraprtl": "css-flip node_modules/bootstrap/dist/css/bootstrap.css > public/assets/lib/bootstrap/css/bootstrap.rtl.css",
    "buildrtl": "npm run build && npm run mainrtl && npm run bootstraprtl"
  },
  "devDependencies": {
    "assemble": "^0.16.2",
    "css-flip": "^0.5.0",
    "grunt": "^1.0.1",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-banner": "^0.6.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-csslint": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-htmlmin": "^2.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-less": "^1.3.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-csscomb": "^3.1.1",
    "grunt-html-validation": "^0.1.18",
    "grunt-modernizr": "^1.0.2",
    "grunt-wiredep": "^3.0.1",
    "gulp-extname": "^0.2.2",
    "handlebars-helpers": "^0.7.3",
    "helper-markdown": "^0.2.1",
    "helper-md": "^0.2.1",
    "jscs": "^3.0.7",
    "jshint-stylish": "^2.2.0",
    "load-grunt-tasks": "^3.5.0",
    "pretty": "^1.0.0",
    "time-grunt": "^1.4.0"
  },
  "dependencies": {
    "animate.css": "^3.5.1",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "metismenu": "^2.5.2",
    "screenfull": "^3.0.0"
  }
}
