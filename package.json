    "bootstraprtl": "css-flip node_modules/bootstrap/dist/css/bootstrap.css > public/assets/lib/bootstrap/css/bootstrap.rtl.css",
    "buildrtl": "npm run build && npm run mainrtl && npm run bootstraprtl"
  },
