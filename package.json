{
  "name": "Bootstrap-Admin-Template",
  "description": "Free Admin Template Based On Twitter Bootstrap 3.x",
  "version": "2.4.0-dev",
  "homepage": "https://github.com/onokumus/Bootstrap-Admin-Template#readme",
  "author": "Osman Nuri Okumu≈ü <onokumus@gmail.com> (https://github.com/onokumus)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onokumus/Bootstrap-Admin-Template.git"
  },
  "bugs": {
    "url": "https://github.com/onokumus/Bootstrap-Admin-Template/issues"
  },
  "license": "MIT",
  "keywords": [
    "twbs",
    "assemble",
    "grunt",
    "less",
    "admin",
    "template"
  ],
  "main": "assemblefile.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "bower": "bower install",
    "grunt": "grunt",
    "build": "npm run bower && npm run grunt",
    "mainrtl": "css-flip dist/assets/css/main.css > dist/assets/css/main.rtl.css",
    "bootstraprtl": "css-flip bower_components/bootstrap/dist/css/bootstrap.css > dist/assets/lib/bootstrap/dist/css/bootstrap.rtl.css",
    "buildrtl": "npm run build && npm run mainrtl && npm run bootstraprtl"
  },
  "devDependencies": {
    "assemble": "^0.9.1",
    "autoprefixer": "^6.3.3",
    "babel-preset-es2015": "^6.6.0",
    "base-watch": "^0.1.2",
    "browser-sync": "^2.11.1",
    "cssnano": "^3.5.2",
    "del": "^2.2.0",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.1",
    "gulp-eslint": "^2.0.0",
    "gulp-extname": "^0.2.2",
    "gulp-header": "^1.7.1",
    "gulp-if": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-prettify": "^0.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-useref": "^3.0.7",
    "helper-link-to": "^0.1.3",
    "helper-markdown": "^0.2.1",
    "helper-md": "^0.1.0",
    "main-bower-files": "^2.11.1",
    "postcss-clearfix": "^0.3.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-reporter": "^1.3.3",
    "postcss-sorting": "^1.2.1",
    "stylelint": "^4.4.0",
    "stylelint-config-standard": "^3.0.0",
    "template-helpers": "^0.5.1",
    "wiredep": "^3.0.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
